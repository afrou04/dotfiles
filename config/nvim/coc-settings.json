{
  "eslint.autoFixOnSave": true,
  "coc.preferences.formatOnType": true,
  "coc.preferences.formatOnSave": true,
  "coc.preferences.formatOnSaveFiletypes": [
    "go",
    "cpp",
    "javascript",
    "javascriptreact",
    "typescript",
    "typescriptreact",
    "json",
    "graphql",
    "handlebars",
    "graphql",
    "prisma",
    "rust"
  ],
  "clangd.path": "/Users/bunta/.config/coc/extensions/coc-clangd-data/install/11.0.0/clangd_11.0.0/bin/clangd",
  "coc.preferences.jumpCommand": "vsplit",
  "languageserver": {
    "golangci-lint-languageserver": {
      "command": "golangci-lint-langserver",
      "filetypes": ["go"],
      "initializationOptions": {
        "command": [
          "golangci-lint",
          "run",
          "--enable-all",
          "--disable",
          "lll",
          "--out-format",
          "json",
          "--issues-exit-code=1"
        ]
      }
    }
  },
  "intelephense": {
    "diagnostics": {
      "undefinedClassConstants": false,
      "undefinedConstants": false,
      "undefinedFunctions": false,
      "undefinedMethods": false,
      "undefinedProperties": false,
      "undefinedTypes": false
    },
    "command": "intelephense",
    "args": ["--stdio"],
    "filetypes": ["php"],
    "initializationOptions": {
      "storagePath": "/tmp/intelephense"
    }
  },
  "git.changedSign.text": "│",
  "git.addedSign.text": "│",
  "git.removedSign.text": "│",
  "git.topRemovedSign.text": "│",
  "git.changeRemovedSign.text": "│",
  "snippets.loadFromExtensions": false,
  "snippets.priority": 120,
  "suggest.languageSourcePriority": 90,
  "sumneko-lua.enableNvimLuaDev": true,
  "coc.source.db.priority": 90,
  "snippets.ultisnips.pythonPrompt": false,
  "cSpell.userWords": [
    "Grpc",
    "Usecase",
    "Youtube",
    "backnumber",
    "cheatsheet",
    "cmds",
    "connectrpc",
    "firestore",
    "intereceptor",
    "queryservice",
    "spwn",
    "ulid",
    "varchar"
  ]
}
